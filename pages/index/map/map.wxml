<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
       style="width: 100%; height: 1334rpx;"
      controls="{{controls}}"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      scale="16"
      covers="{{covers}}"
      bindcontroltap="moveToLocation" 
      show-location
    ></map>
  </view>

  
  
</view>
